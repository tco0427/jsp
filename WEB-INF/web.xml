<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>HelloWorldServlet</servlet-name>
        <servlet-class>ch04.HelloWorldServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalcServlet</servlet-name>
        <servlet-class>ch04.CalcServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalcServlet2</servlet-name>
        <servlet-class>ch04.CalcServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CalcServlet2</servlet-name>
        <url-pattern>/CalcServlet2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HelloWorldServlet</servlet-name>
        <url-pattern>/HelloWorldServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalcServlet</servlet-name>
        <url-pattern>/CalcServlet</url-pattern>
    </servlet-mapping>
</web-app>
